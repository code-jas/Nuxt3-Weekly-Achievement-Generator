{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "productionBranch": "prod",
    "deployments": {
      "previewBranchSuffix": "-preview",
      "allowedBranches": ["feature/*", "bugfix/*", "release/*", "master"]
    }
  }
}
